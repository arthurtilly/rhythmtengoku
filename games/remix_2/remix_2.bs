struct remix_2_marking_criteria_0
    /* Positive Remark */     .word D_08067edc
    /* Negative Remark */     .word D_08067ecc
    /* Flags */               .hword CHECK_AVERAGE_HITS | CHECK_AVERAGE_MISSES
