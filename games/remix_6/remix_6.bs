    run stop_all_soundplayers
    rest 24
    play_sfx s_jingle_remix_seqData
    rest 24
    rest 24
    run func_08046bec, 1
    rest 2
    run func_08046bec, 2
    rest 2
    run func_08046bec, 3
    rest 2
    run func_08046bec, 4
    rest 2
    run func_08046bec, 5
    rest 2
    run func_08046bec, 6
    rest 6
    rest 6
    rest 24
    rest 24
    rest 12
